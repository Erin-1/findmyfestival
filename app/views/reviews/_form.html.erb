<div class="banner-bottom">

<%= simple_form_for([@booking, @review]) do |f| %>

  <div class="form-group">
    <%= f.label :content %>
    <%= f.text_field :content, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :stars %>
    <%= f.text_field :stars, class: 'form-control' %>
  </div>

  <%= f.submit class: 'btn btn-primary' %>

<% end %>


</div>

